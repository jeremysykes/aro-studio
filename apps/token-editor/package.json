{
  "name": "token-editor",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./electron-entry.cjs",
  "scripts": {
    "dev": "pnpm exec tsc -p tsconfig.preload.json && mv dist-electron/preload/index.js dist-electron/preload/index.cjs && concurrently -k \"vite\" \"wait-on http://localhost:5174 && NODE_ENV=development electron .\"",
    "electron": "electron .",
    "build": "tsc && vite build && tsc -p tsconfig.electron.json",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@adobe/react-spectrum": "^3.46.0",
    "@aro-studio/core": "workspace:*",
    "@aro-studio/ui": "workspace:*",
    "@monaco-editor/react": "^4.6.0",
    "@spectrum-icons/workflow": "^4.2.26",
    "@tanstack/react-table": "^8.20.5",
    "@tanstack/react-virtual": "^3.13.18",
    "electron": "28.3.3",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zundo": "^2.3.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "wait-on": "^7.2.0"
  }
}
